{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/node/index.js",
  "typings": "dist/node/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node dist/train",
    "build": "yarn build:js && yarn build:mql",
    "build:js": "yarn build:js:main && yarn build:js:train && yarn minify && yarn copy && yarn rm:file",
    "build:js:main": "yarn ncc build src/ml/index.ts --minify --no-cache --target es2020 -o dist/index",
    "build:js:train": "yarn ncc build src/ml/train.ts --minify --no-cache --target es2020 -o dist/train",
    "minify": "yarn minify:main && yarn minify:train",
    "minify:main": "terser --compress --mangle --comments=false --output=dist/index/main.js dist/index/index.js",
    "minify:train": "terser --compress --mangle --comments=false --output=dist/train/train.js dist/train/index.js",
    "copy": "yarn copy:deps && yarn copy:lib",
    "copy:deps": "copyfiles -f ./node_modules/@tensorflow/tfjs-node/package.json ./dist/index/main.js ./dist/train/train.js dist",
    "copy:lib": "copyfiles -u 3 ./node_modules/@tensorflow/tfjs-node/lib/**/*.* dist",
    "rm:file": "rimraf dist/index dist/train",
    "build:mql": "cd scripts && build"
  },
  "name": "aurum-79",
  "author": "Ribhararnus Pracutian",
  "module": "dist/ml.esm.js",
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "@types/mkdirp": "^1.0.1",
    "@types/rimraf": "^3.0.0",
    "@vercel/ncc": "^0.25.1",
    "copyfiles": "^2.4.1",
    "terser": "^5.5.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^2.7.0",
    "@tensorflow/tfjs-node": "^2.7.0",
    "decimal.js": "^10.2.1",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "nanoid": "^3.1.20",
    "ora": "^5.1.0",
    "rimraf": "^3.0.2",
    "wait-port": "^0.2.9"
  }
}
