{
  "include": ["src", "types"],
  "compilerOptions": {
    "esModuleInterop": true,
    "importHelpers": true,
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "rootDir": "src/ml",
    "sourceMap": true,
    "strict": true,
    "target": "esnext"
  }
}
